{
  "name": "secoli-bui-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "check:no-eslint-disable": "./scripts/check-no-debugger-or-console.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "axios": "^0.26.1",
    "storybook": "^6.4.21",
    "vue": "^3.2.31",
    "vue-axios": "^3.4.1",
    "vue-router": "^4.0.13",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/vue3": "^6.4.21",
    "@vitejs/plugin-vue": "^2.3.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.6.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "sass": "^1.50.0",
    "storybook-builder-vite": "^0.1.23",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.4",
    "url": "^0.11.0",
    "vite": "^2.9.0",
    "vite-plugin-components": "^0.13.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.29.8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "vue-cli-service lint",
      "npm run check:no-eslint-disable",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  }
}
